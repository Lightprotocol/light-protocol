{
  "name": "@lightprotocol/compressed-token",
  "version": "0.0.1",
  "description": "JS client to interact with the compressed-token program",
  "main": "lib/index.js",
  "type": "module",
  "scripts": {
    "test": "pnpm run test:program",
    "test-all": "vitest run",
    "test-validator:silent": "./../../cli/test_bin/run test-validator -b",
    "test-validator": "./../../cli/test_bin/run test-validator",
    "test:program": "pnpm run test-validator:silent && vitest run tests/e2e/program.test.ts",
    "emit-event:create_mint": "vitest run tests/e2e/emit-create_mint.test.ts",
    "emit-event:mint_to": "vitest run tests/e2e/emit-mint_to-event.test.ts",
    "test:verbose": "vitest run --reporter=verbose",
    "build": "rimraf lib && pnpm run pull-idl && pnpm tsc",
    "pull-idl": "../../scripts/push-compressed-token-idl.sh"
  },
  "keywords": [
    "zk",
    "compression",
    "zk-compression",
    "stateless",
    "solana"
  ],
  "author": "",
  "license": "Apache-2.0",
  "devDependencies": {
    "@types/node": "^20.10.2",
    "rimraf": "^5.0.5",
    "ts-node": "^10.9.1",
    "typescript": "5.3.2",
    "vitest": "^0.34.6"
  },
  "dependencies": {
    "@coral-xyz/anchor": "0.28.0",
    "@lightprotocol/stateless.js": "workspace:*",
    "@metaplex-foundation/beet": "0.7.1",
    "@metaplex-foundation/beet-solana": "^0.4.1",
    "@solana/spl-token": "^0.3.7",
    "@solana/web3.js": "^1.87.6",
    "buffer": "^6.0.3",
    "tweetnacl": "^1.0.3"
  }
}
