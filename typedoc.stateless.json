{
  "$schema": "https://typedoc.org/schema.json",
  "name": "@lightprotocol/stateless.js",
  "entryPoints": ["js/stateless.js/src/index.ts"],
  "out": "api-docs/stateless.js",
  "exclude": [
    "**/node_modules/**",
    "**/*.test.ts",
    "**/*.spec.ts",
    "**/tests/**",
    "**/dist/**",
    "**/playwright.config.ts"
  ],
  "excludePrivate": true,
  "excludeProtected": false,
  "excludeInternal": true,
  "skipErrorChecking": true,
  "readme": "js/stateless.js/README.md",
  "tsconfig": "js/stateless.js/tsconfig.json",
  "compilerOptions": {
    "skipLibCheck": true
  },
  "plugin": [],
  "navigation": {
    "includeCategories": true,
    "includeGroups": true
  },
  "categorizeByGroup": true,
  "sort": ["source-order"],
  "kindSortOrder": [
    "Function",
    "Class",
    "Interface",
    "TypeAlias",
    "Variable",
    "Enum"
  ],
  "searchInComments": true,
  "visibilityFilters": {
    "protected": true,
    "private": false,
    "inherited": true,
    "external": false
  },
  "hideGenerator": false,
  "includeVersion": true,
  "githubPages": true,
  "navigationLinks": {
    "Other Libraries": "../index.html",
    "Documentation": "https://www.zkcompression.com",
    "GitHub": "https://github.com/Lightprotocol/light-protocol"
  }
}
