{
  "name": "lightprotocol-cli",
  "version": "0.0.1",
  "description": "Light Protocol core cli",
  "author": "sleepyqadir @sleepyqadir shigoto-dev19",
  "bin": {
    "light": "./bin/run"
  },
  "license": "MIT",
  "main": "dist/index.js",
  "files": [
    "/bin",
    "/dist",
    "./config.json",
    "/npm-shrinkwrap.json",
    "/oclif.manifest.json"
  ],
  "dependencies": {
    "@coral-xyz/anchor": "^0.27.0",
    "@lightprotocol/zk.js": "^0.3.2-alpha.8",
    "@oclif/core": "^2",
    "@oclif/plugin-plugins": "^3.0.1",
    "@solana/spl-token": "^0.3.7",
    "@solana/web3.js": "^1.76.0",
    "@typescript-eslint/eslint-plugin": "^5.59.7",
    "@typescript-eslint/parser": "^5.59.7",
    "cli-spinners": "^2.9.0",
    "dotenv": "^16.0.3",
    "fs": "^0.0.1-security",
    "path": "^0.12.7",
    "ts-mocha": "^10.0.0"
  },
  "devDependencies": {
    "@oclif/errors": "^1.3.6",
    "@oclif/plugin-autocomplete": "^2.3.0",
    "@oclif/plugin-not-found": "^2.3.26",
    "@oclif/test": "^2.3.17",
    "@types/bn.js": "^5.1.1",
    "@types/chai": "^4.3.5",
    "@types/mocha": "^9.0.0",
    "@types/node": "^16.18.26",
    "chai": "^4.3.7",
    "eslint": "^8.41.0",
    "eslint-config-oclif": "^4",
    "eslint-config-oclif-typescript": "^1.0.3",
    "mocha": "^9",
    "oclif": "^3",
    "shx": "^0.3.3",
    "ts-node": "^10.9.1",
    "tslib": "^2.5.0",
    "typescript": "^4.9.5",
    "why-is-node-running": "^2.2.2",
    "wtfnode": "^0.9.1"
  },
  "oclif": {
    "bin": "light",
    "dirname": "light",
    "commands": "./dist/commands",
    "plugins": [
      "@oclif/plugin-help",
      "@oclif/plugin-plugins",
      "@oclif/plugin-autocomplete",
      "@oclif/plugin-not-found"
    ],
    "topicSeparator": ""
  },
  "scripts": {
    "build": "shx rm -rf dist && tsc -b",
    "lint": "eslint . --ext .ts --config .eslintrc",
    "postpack": "shx rm -f oclif.manifest.json",
    "posttest": "yarn lint",
    "prepack": "yarn build && oclif manifest && oclif readme",
    "test": "mocha --forbid-only \"test/**/*.test.ts\"",
    "test-account": "mocha --reporter spec ./test/commands/account/index.test.ts",
    "test-balance": "mocha ./test/commands/balance/index.test.ts",
    "test-user": "mocha ./test/commands/shield/index.test.ts",
    "test-config": "mocha ./test/commands/psp-config/index.test.ts",
    "test-airdrop": "mocha --bail --forbid-only ./test/commands/airdrop/index.test.ts --exit",
    "test-shield1": "mocha ./test/commands/shield/shield1.test.ts --exit",
    "test-shield2": "mocha ./test/commands/shield/shield2.test.ts --exit",
    "test-shield:sol": "mocha ./test/commands/shield/shield:sol.test.ts --exit",
    "test-unshield1": "mocha ./test/commands/unshield/unshield1.test.ts --exit",
    "test-unshield2": "mocha ./test/commands/unshield/unshield2.test.ts --exit",
    "test-unshield:sol": "mocha ./test/commands/unshield/unshield:sol.test.ts --exit",
    "test-unshield": "yarn test-unshield1 && yarn test-unshield2 && yarn test-unshield:sol",
    "test-accept_utxos": "mocha ./test/commands/accept-utxos/index.test.ts --exit",
    "test-transfer": "mocha ./test/commands/transfer/index.test.ts --exit",
    "test-cli": "ts-node ./test/cli.test.ts",
    "test-cli-sdk": "./bin/run test-validator && mocha ./test/cli-sdk.test.ts --exit",
    "version": "oclif readme && git add README.md"
  },
  "engines": {
    "node": ">=12.0.0"
  },
  "keywords": [
    "oclif"
  ],
  "types": "dist/index.d.ts"
}
