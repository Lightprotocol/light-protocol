{
  "name": "@lightprotocol/cli",
  "version": "0.1.1-alpha.20",
  "description": "Light Protocol CLI",
  "author": "@ananas-block, @shigoto-dev19, @sleepyqadir ",
  "bin": {
    "light": "./test_bin/run"
  },
  "license": "MIT",
  "main": "dist/index.js",
  "files": [
    "/accounts",
    "/dist",
    "./config.json",
    "/npm-shrinkwrap.json",
    "/oclif.manifest.json"
  ],
  "dependencies": {
    "@coral-xyz/anchor": "0.28.0",
    "@lightprotocol/zk.js": "file:../light-zk.js",
    "@oclif/core": "^2",
    "@oclif/plugin-autocomplete": "^2.3.4",
    "@oclif/plugin-help": "^5.2.15",
    "@oclif/plugin-not-found": "^2.3.34",
    "@oclif/plugin-plugins": "^3.0.1",
    "@project-serum/anchor": "^0.26.0",
    "@solana/spl-token": "^0.3.7",
    "@solana/web3.js": "^1.76.0",
    "@typescript-eslint/eslint-plugin": "^5.59.7",
    "@typescript-eslint/parser": "^5.59.7",
    "chai": "^4.3.7",
    "cli-progress": "^3.12.0",
    "cli-spinners": "^2.9.0",
    "dotenv": "^16.0.3",
    "ffjavascript": "0.2.60",
    "fs": "^0.0.1-security",
    "node-fetch": "^3.3.1",
    "path": "^0.12.7",
    "snake-case": "^3.0.4",
    "tar": "^6.1.15",
    "ts-mocha": "^10.0.0",
    "zlib": "^1.0.5"
  },
  "devDependencies": {
    "@oclif/dev-cli": "^1.26.10",
    "@oclif/errors": "^1.3.6",
    "@oclif/test": "^2.3.17",
    "@types/bn.js": "^5.1.1",
    "@types/chai": "^4.3.5",
    "@types/mocha": "^10.0.1",
    "@types/node": "^16.18.26",
    "@types/tar": "^6.1.5",
    "chai": "^4.3.7",
    "eslint": "^7.5.0",
    "eslint-config-oclif": "^4",
    "eslint-config-oclif-typescript": "^1.0.3",
    "mocha": "^10.2.0",
    "oclif": "^3.10.0",
    "prettier": "^2.6.2",
    "shx": "^0.3.3",
    "ts-node": "^10.9.1",
    "tslib": "^2.5.0",
    "typescript": "^4.9.5"
  },
  "oclif": {
    "bin": "light",
    "dirname": "light",
    "commands": "./dist/commands",
    "plugins": [
      "@oclif/plugin-help",
      "@oclif/plugin-plugins",
      "@oclif/plugin-autocomplete",
      "@oclif/plugin-not-found"
    ],
    "topicSeparator": ""
  },
  "scripts": {
    "build": "shx rm -rf dist && yarn tsc -b && sh scripts/copyLocalProgramBinaries.sh",
    "format": "yarn prettier --write \"src/**/*.{ts,js}\" \"test/**/*.{ts,js}\" -w",
    "lint": "yarn prettier \"src/**/*{ts,js}\"  \"test/**/*.{ts,js}\" --check",
    "postpack": "shx rm -f oclif.manifest.json",
    "prepack": "yarn build && oclif manifest && oclif readme",
    "test-utils": "mocha ./test/utils/psp-utils/index.test.ts --exit",
    "test-account": "mocha ./test/commands/account/index.test.ts --exit",
    "test-balance": "mocha ./test/commands/balance/index.test.ts",
    "test-config": "mocha ./test/commands/psp-config/index.test.ts",
    "test-airdrop": "mocha ./test/commands/airdrop/index.test.ts --exit",
    "test-shield": "mocha ./test/commands/shield/shield.test.ts --exit",
    "test-shield:sol": "mocha ./test/commands/shield/shield:sol.test.ts --exit",
    "test-shield:spl": "mocha ./test/commands/shield/shield:spl.test.ts --exit",
    "test-unshield": "mocha ./test/commands/unshield/unshield.test.ts --exit",
    "test-unshield:sol": "mocha ./test/commands/unshield/unshield:sol.test.ts --exit",
    "test-unshield:spl": "mocha ./test/commands/unshield/unshield:spl.test.ts --exit",
    "test-accept-utxos": "mocha ./test/commands/accept-utxos/index.test.ts --exit",
    "test-transfer": "mocha ./test/commands/transfer/index.test.ts --exit",
    "kill": "killall solana-test-validator || true && killall solana-test-val || true && sleep 1",
    "test-cli": "yarn test-account && yarn test-config && yarn test-airdrop && yarn test-shield && yarn test-shield:sol && yarn test-shield:spl && yarn test-unshield && yarn test-unshield:sol && yarn test-unshield:spl && yarn test-transfer && yarn test-accept-utxos && yarn kill",
    "test-psp-init": "cd .. && rm -r tmp-test-psp || true && ./cli/test_bin/run psp:init tmp-test-psp && cd cli",
    "test-psp-add-circuit": "cd ../tmp-test-psp && ./../cli/test_bin/run psp:add-circuit second-circuit --circom && ./../cli/test_bin/run psp:add-circuit second-circuit1 && cd ../cli && ts-node scripts/checkAddCircuit.ts ../tmp-test-psp second-circuit true && ts-node scripts/checkAddCircuit.ts ../tmp-test-psp second-circuit1",
    "test-psp-build": "cd ../tmp-test-psp && ./../cli/test_bin/run psp:build tmp-test-psp && cd ../cli",
    "test-psp-test": "cd ../tmp-test-psp && ./../cli/test_bin/run psp:test tmp-test-psp Fg6PaFpoGXkYsidMpWTK6W2BeZ7FEfcYkg476zPFsLnS && cd ../cli",
    "clean-test-psp": "rm -r ../tmp-test-psp || true",
    "test-psp": "yarn test-psp-init && yarn test-psp-build && yarn test-psp-test && yarn test-psp-add-circuit",
    "test-circom-init": "cd .. && rm -r tmp-test-circom || true && ./cli/test_bin/run circom:init tmp-test-circom && cd cli",
    "test-circom-add-circuit": "cd ../tmp-test-circom && ./../cli/test_bin/run circom:add-circuit second-circuit && cd ../cli && ts-node scripts/checkAddCircuit.ts ../tmp-test-circom second-circuit true",
    "test-circom-build": "cd ../tmp-test-circom && ./../cli/test_bin/run circom:build tmp-test-circom && cd ../cli",
    "test-circom-test": "cd ../tmp-test-circom && ./../cli/test_bin/run circom:test tmp-test-circom Fg6PaFpoGXkYsidMpWTK6W2BeZ7FEfcYkg476zPFsLnS && cd ../cli",
    "clean-test-circom": "rm -r ../tmp-test-circom || true",
    "test-circom": "yarn test-circom-init && yarn test-circom-build && yarn test-circom-test && yarn test-circom-add-circuit",
    "test": "yarn kill && yarn test-cli && yarn test-psp && yarn test-circom",
    "install-local": "yarn build && yarn global remove @lightprotocol/cli || true && yarn global add $PWD",
    "version": "oclif readme && git add README.md"
  },
  "engines": {
    "node": ">=12.0.0"
  },
  "keywords": [
    "oclif"
  ],
  "types": "dist/index.d.ts"
}
