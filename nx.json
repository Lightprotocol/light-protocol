{
  "namedInputs": {
    "noMarkdown": [
      "!{projectRoot}/**/*.md"
    ],
    "noTestLedger": [
      "!{projectRoot}/test-ledger"
    ]
  },
  "targetDefaults": {
    "build": {
      "cache": true,
      "inputs": [
        "noMarkdown",
        "^noMarkdown",
        "noTestLedger",
        "^noTestLedger"
      ],
      "dependsOn": [
        "^build"
      ],
      "outputs": [
        "{workspaceRoot}/{projectRoot}",
        "{workspaceRoot}/target/deploy",
        "{workspaceRoot}/target/idl",
        "{workspaceRoot}/target/types"
      ]
    },
    "test": {
      "cache": true,
      "inputs": [
        "noMarkdown",
        "^noMarkdown",
        "noTestLedger",
        "^noTestLedger"
      ],
      "dependsOn": [
        "^build",
        "build"
      ]
    },
    "format": {
      "cache": true,
      "inputs": [
        "noMarkdown",
        "^noMarkdown",
        "noTestLedger",
        "^noTestLedger"
      ]
    },
    "format:check": {
        "cache": true,
        "inputs": [
            "noMarkdown",
            "^noMarkdown",
            "noTestLedger",
            "^noTestLedger"
        ]
    },
    "lint": {
      "cache": true,
      "inputs": [
        "noMarkdown",
        "^noMarkdown",
        "noTestLedger",
        "^noTestLedger"
      ]
    }
  },
